{
  "name": "@xwtec/dotfiles",
  "version": "0.0.2",
  "description": "xwtec project config files",
  "main": "index.js",
  "keywords": ["xwtec", "config"],
  "repository": "xwtec/dotfiles",
  "author": "fisker <lionkay@gmail.com>",
  "license": "MIT",
  "files": [
    "index.js",
    "README.md",
    "prettier/index.js",
    "prettier/prettier.config.js"
  ],
  "scripts": {
    "release": "np --no-yarn --no-cleanup",
    "prettier": "prettier **/*.{js,json,md} --write"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "cz-conventional-changelog-emoji": "0.1.0",
    "husky": "3.0.4",
    "lint-staged": "9.2.2",
    "np": "5.0.3",
    "prettier": "1.18.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  }
}
