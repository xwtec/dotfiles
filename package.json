    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "np": "^4.0.2",
    "prettier": "^1.16.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
