{
  "name": "@xwtec/dotfiles",
  "version": "0.0.2",
  "description": "xwtec project config files",
  "main": "index.js",
  "keywords": ["xwtec", "config"],
  "repository": "xwtec/dotfiles",
  "author": "fisker <lionkay@gmail.com>",
